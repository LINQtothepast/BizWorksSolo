<Window x:Class="BizWorks.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BizWorks"
        mc:Ignorable="d"
        Title="Window1" Height="550" Width="700">
    <Grid>
        <TextBox x:Name="Username_Textbox" GotFocus="TextBox_GotFocus" HorizontalAlignment="Left" Height="23" Margin="290,119,0,0" TextWrapping="Wrap" Text="admin" VerticalAlignment="Top" Width="120" />
        <Label x:Name="label" Content="Employee Login" HorizontalAlignment="Left" Height="29" Margin="290,77,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="Password_Textbox" GotFocus="TextBox_GotFocus" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="password" VerticalAlignment="Top" Width="120" Margin="290,165,0,0" />
        <Button x:Name="button" Content="Login" HorizontalAlignment="Left" Margin="290,224,0,0" VerticalAlignment="Top" Width="120" Click="button_Click"/>

    </Grid>
</Window>
